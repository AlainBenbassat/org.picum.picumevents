<?xml version="1.0"?>
<extension key="org.picum.picumevents" type="module">
  <file>picumevents</file>
  <name>PICUM Events</name>
  <description>event related customization</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/AlainBenbassat/org.picum.picumevents</url>
  </urls>
  <releaseDate>2020-06-10</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Custom public participant listings + checksum check</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Picumevents</namespace>
    <format>22.10.0</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
